    <ParagraphTuple sequenceId="8" langs="en_ja">
      <Paragraph sequenceId="8" lang="en" timeSpan="0:0:0:0_0:0:0:0" style="Regular">
        <Content sequenceId="8" lang="en">
          <Text sequenceId="8" lang="en">
            <RegularRun fontStyle="Regular" text="Urashima Taro took the treasure box, got onto the turtle’s back and set off towards his village." />
          </Text>
